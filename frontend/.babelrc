{
    "presets": [
        "react",
        ["es2015", {"modules": false, "loose": true}]
    ],
    "env": {
        "development": {
            "plugins": [
                ["transform-async-to-generator"],
                ["transform-class-properties"],
                ["transform-object-rest-spread"],
                ["syntax-export-extensions"],
                ["transform-export-extensions"],
                ["react-hot-loader/babel"]
            ]
        },
        "production": {
            "plugins": [
                ["transform-async-to-generator"],
                ["transform-class-properties"],
                ["transform-object-rest-spread"],
                ["syntax-export-extensions"],
                ["transform-export-extensions"]
            ]
        }
    }
}
